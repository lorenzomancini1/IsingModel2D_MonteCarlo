    "#--- Computing τ for different LATTICE SIZE N at T=Tc -----------\n",
    "\n",
    "N_n = np.arange(50,151,50)\n",
    "N_N=len(N_n)\n",
    "\n",
    "eq_steps = int(5e2)\n",
