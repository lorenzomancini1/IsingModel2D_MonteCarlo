    "\n",
    "E_mean, M_mean, C_mean, Chi_mean, E_var, M_var, E_eq, M_eq, E, M_G,  = Ising(N, beta, eq_steps=int(1e3), steps=int(1e3))\n",
    "E_mean, M_mean, C_mean, Chi_mean, E_var, M_var, E_eq, M_eq, E, M_W,  = Ising(N, beta, eq_steps=int(1e3), steps=int(1e3), cluster=True)\n",
    "\n",
    "tmpG = acf(M_G, nlags=200, fft = False) \n",
    "tmpW = acf(M_W, nlags=200, fft = False)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 18,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "\n",
      "text/plain": [
       "<Figure size 864x432 with 1 Axes>"
      ]
     },
     "metadata": {
      "needs_background": "light"
     },
     "output_type": "display_data"
    }
   ],
   "source": [
    "plt.figure(figsize=(12,6))\n",
    "plt.plot(tmpW, label='Wolff', color='blue')\n",
    "plt.plot(tmpG, label='Glauber', color='red')\n",
    "plt.title(r'Autocorrelation Glauber vs Wolff (T = $T_c$)', fontsize=20, fontweight = \"bold\")\n",
    "plt.xlabel('lag time', fontsize=18)\n",
    "plt.ylabel('Acf', fontsize=18)\n",
    "plt.axhline(0, c = \"black\", linestyle = \"--\", linewidth = 0.8, alpha = 0.8)\n",
    "plt.xlim([-10,200])\n",
    "#plt.grid()\n",
    "plt.legend()\n",
    "plt.show()"
   ]
